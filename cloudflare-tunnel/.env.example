# ============================================
# CLOUDFLARE TUNNEL ENVIRONMENT VARIABLES
# ============================================

# Primary tunnel for most services
CLOUDFLARE_TUNNEL_TOKEN=your_primary_cloudflare_tunnel_token_here

# Backup tunnel for dedicated services (optional)
CLOUDFLARE_TUNNEL_BACKUP_TOKEN=your_backup_cloudflare_tunnel_token_here

# Tunnel Configuration
TUNNEL_ID=your_tunnel_id_here
TUNNEL_NAME=homelab-tunnel

# Domain Configuration
DOMAIN=your-homelab-domain.com